<h1 class="mt-4">Admin Dashboard</h1>
<p class="lead mb-3">Welcome <%= user.name %>
</p>
<% if (activeSessions) { %>
    <h2 class="text-info">Current Active Sessions: <%= activeSessions.length %></h2>
    <% for (let session of activeSessions) { %>
        <p>SessionID: <%= session.sessionId %></p>
        <p>UserID: <%= session.userId %></p>
        <a href=""></a>
        <a href="<%= "/revokeSession?sessionId=" + session.sessionId %>" class="bg-warning btn btn-outline-warining">Revoke Session</a>
    <% } %>
<% } else { %>
    <p>Something is wrong, no session found</p>
<% } %>